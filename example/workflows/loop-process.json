{
  "name": "循环流程",
  "description": "包含退回和重新提交的循环流程",
  "nodes": [
    {
      "nodeId": "start",
      "nodeName": "开始",
      "nodeType": "start",
      "next": "submit"
    },
    {
      "nodeId": "submit",
      "nodeName": "提交资料",
      "nodeType": "process",
      "next": "review"
    },
    {
      "nodeId": "review",
      "nodeName": "资料审核",
      "nodeType": "approval",
      "actions": [
        { "actionName": "通过", "next": "approve" },
        { "actionName": "退回修改", "next": "submit" }
      ]
    },
    {
      "nodeId": "approve",
      "nodeName": "最终审批",
      "nodeType": "approval",
      "actions": [
        { "actionName": "批准", "next": "end" },
        { "actionName": "驳回", "next": "submit" }
      ]
    },
    {
      "nodeId": "end",
      "nodeName": "完成",
      "nodeType": "end"
    }
  ]
}

